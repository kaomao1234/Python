<HomePage>:
    MDBoxLayout:
        orientation:'vertical'
        MDTopAppBar:
            elevation:2.5
            title:"Appbar"
        MDScreen:
            MDBoxLayout:
                orientation:'vertical'
                MDTextButton:
                    text:"Clicked"
                    on_press:
                        root.on_press()
                AnchorLayout:
                    anchor_y:'bottom'
                    MDBottomAppBar:
                        MDTopAppBar:
                            title: "Title"
                            icon: "git"
                            type: "bottom"
                            left_action_items: [["menu", lambda x: x]]
                            mode: "center"
                            elevation:0


